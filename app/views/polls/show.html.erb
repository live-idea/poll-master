<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', polls_path  %></li>
      <li><%= link_to 'New', new_poll_path %></li>
      <li class="active"><%= link_to 'Show', poll_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Title:</b>
          <%= @poll.title %>
        </p>

        <p>
          <b>Branch:</b>
          <%= @poll.branch %>
        </p>

        <p>
          <b>Location:</b>
          <%= @poll.location %>
        </p>

        <p>
          <b>State:</b>
          <%= @poll.state %>
        </p>

        <p>
          <b>Description:</b>
          <%= @poll.description %>
        </p>

        <p>
          <b>Start_date:</b>
          <%= @poll.start_date %>
        </p>

        <p>
          <b>End_date:</b>
          <%= @poll.end_date %>
        </p>

        <p>
          <b>User_id:</b>
          <%= @poll.user_id %>
        </p>

      <%= link_to "#{image_tag("web-app-theme/application_edit.png", :alt => "Edit")} Edit", edit_poll_path(@poll), :class => "button" %> 
      <%= link_to "#{image_tag("web-app-theme/cross.png", :alt => "Edit")} Delete", poll_path(@poll), :method => "delete", :class => "button", :confirm => "Are you sure?" %> 
      <div class="clear"></div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>