<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', results_path  %></li>
      <li><%= link_to 'New', new_result_path %></li>
      <li class="active"><%= link_to 'Show', result_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Anketa_id:</b>
          <%= @result.anketa_id %>
        </p>

        <p>
          <b>Responder_id:</b>
          <%= @result.responder_id %>
        </p>

        <p>
          <b>Percentage:</b>
          <%= @result.percentage %>
        </p>

        <p>
          <b>Responder_ip:</b>
          <%= @result.responder_ip %>
        </p>

        <p>
          <b>Comment:</b>
          <%= @result.comment %>
        </p>

      <%= link_to "#{image_tag("web-app-theme/application_edit.png", :alt => "Edit")} Edit", edit_result_path(@result), :class => "button" %> 
      <%= link_to "#{image_tag("web-app-theme/cross.png", :alt => "Edit")} Delete", result_path(@result), :method => "delete", :class => "button", :confirm => "Are you sure?" %> 
      <div class="clear"></div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>