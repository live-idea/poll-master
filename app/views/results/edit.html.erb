<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li><%= link_to t(:new_anketa), new_result_path %></li>
      <li class="active"><%= link_to t(:edit), edit_result_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">            
    <h2 class="title"></h2>
    <div class="inner">
      <% form_for @result, :url => poll_anketa_result_path(@poll,@anketa,@result), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>