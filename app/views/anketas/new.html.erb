<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to t(:anketa_all), poll_anketas_path(@poll)  %></li>
      <li class="active"><%= link_to t(:anketa_new), new_poll_anketa_path(@poll) %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">            
    <h2 class="title"><%=t :one_new_anketa %></h2>
    <div class="inner">
      <%= error_messages_for :anketa%>
      <% form_for :anketa, :url => poll_anketas_path(@poll), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>