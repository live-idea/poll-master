<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', poll_anketas_path(@poll)  %></li>
      <li><%= link_to 'New', new_poll_anketa_path(@poll) %></li>
      <li class="active"><%= link_to 'Edit', edit_poll_anketa_path(@poll) %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">            
    <h2 class="title">Edit Anketa</h2>
    <div class="inner">
      <% form_for @anketa, :url => poll_anketa_path(@poll,@anketa), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>